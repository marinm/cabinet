{
    "manifest_version": 2,
    "name": "cabinet",
    "version": "0.0.0",

    "description": "Adds a red border to all webpages matching mozilla.org.",

    "icons": {
        "48": "icons/48.png"
    },

    "content_scripts": [
        {
            "matches": ["*://*.mozilla.org/*"],
            "js": ["cabinet.js"]
        }
    ]
} 